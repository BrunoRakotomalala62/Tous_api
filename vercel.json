{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/claude(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/minilm(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/reset(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api-info(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
